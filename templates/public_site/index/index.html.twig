{% extends 'public_site/base.html.twig' %}

{% block content %}
    {% if blockProvider.count > 0 %}
        {% for blck in page.pageBlocks %}
            {% include 'public_site/block.html.twig' with { 'block_template' : blockProvider.currentTypeManager.twigTemplate, 'block_data' : blck, 'data': blockProvider.objNext} %}
        {% endfor %}
    {% endif %}
{% endblock %}